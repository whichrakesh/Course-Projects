float func(){

	float a[5];
	float b[5];
	int i;
	float sum;

	for(i=0;i<5;i++){
		a[i] = 0.5;
		b[i] = 0.5;
	}

	sum = 0;
	for(i=0;i<5;i++){
		sum = sum + a[i]* b[i];
	}
	printf(sum);
	return sum;

}

int l(){
	float ans;
	ans = func();
	return 0;
}

int yf(){

	int x;
	int y;
	int z;
	int a;
	x = 2;
	y = 3;
	z = 2;
	a = x >y;
	a = x < y;
	a = x <= y;
	a = x >= y;
	a = x == z;
	a = x != z;
	a = x - y - z;


}

int main(){
	int x;
	int y;
	int z;
	float j;
	float k;
	x = 1234;
	y = 0;
	z = 0;
	k =0;
	while(x != 0){
		y = x/10;
		y = x - y*10;
		z = z*10 + y;
		x = x/10;
		for(j=0;j<1;j++){
			k = z + func();
		}
	}
	j = k;
}